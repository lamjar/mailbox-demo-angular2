  "dependencies": {
    "lodash": "^4.13.1",
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "core-js": "^2.2.0",
    "ie-shim": "^0.1.0",
    "rxjs": "5.0.0-beta.6"
  },
  "description": "mailbox showcase angular2",
  "devDependencies": {
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.3",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^1.1.1",
    "css-loader": "^0.23.1",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "typescript": "~1.8.9",
    "typings": "~1.0.3",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.9.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "engines": {
    "node": "^4.4.0"
  },
  "license": "LicenseRef-LICENSE",
  "main": "index.js",
  "name": "@zalari/mailbox-demo-angular2",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "buildDebug": "webpack -d",
    "clean": "rimraf dist/*",
    "copy": "mkdirp dist/assets && cp -r src/main/assets dist/",
    "lint": "jshint $1",
    "lintAll": "jshint src/main/js",
    "package": "npm run clean && npm run copy && npm run build",
    "packageDebug": "npm run clean && npm run copy && npm run buildDebug",
    "serve": "webpack-dev-server --content-base src/main/ --hot --inline -v",
    "serve:dist": "npm run package && http-server dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "version": "0.0.1",
  "private": true
